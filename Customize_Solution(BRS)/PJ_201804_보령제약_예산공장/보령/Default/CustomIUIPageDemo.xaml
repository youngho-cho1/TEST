<UserControl x:Class="Site.CustomIUIPageDemo"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:icommon="clr-namespace:LGCNS.iPharmMES.Common;assembly=LGCNS.iPharmMES.Common">
    <icommon:iPharmBusyIndicator IsBusyMVVM="{Binding IsBusy}">
        <Grid x:Name="LayoutRoot" Background="White">
            <TextBlock FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center" Text="IUI CustomPage"/>
        </Grid>
    </icommon:iPharmBusyIndicator>
</UserControl>
