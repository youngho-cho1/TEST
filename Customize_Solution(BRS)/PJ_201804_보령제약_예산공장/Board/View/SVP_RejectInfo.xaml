                                <c1:DataGridTextColumn SortMemberPath="COMMENTS" Header="[비고, 비고, 비고, 비고, 비고, 비고]" Binding="{Binding COMMENTS}" HorizontalAlignment="Left" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
