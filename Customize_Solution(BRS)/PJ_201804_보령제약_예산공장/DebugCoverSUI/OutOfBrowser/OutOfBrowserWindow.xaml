<UserControl x:Class="DebugCoverSUI.OutOfBrowserWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="200" d:DesignWidth="300">

    <Grid x:Name="LayoutRoot" Background="White">
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBlock Name="_TextMessage1" Text="Please, Wait..." HorizontalAlignment="Center"/>
            <TextBlock Name="_TextMessage2" Text="Application is launching" HorizontalAlignment="Center"/>
            <Button Name="_ButtonExit" Content="OK" Click="_ButtonExit_Click" Width="60" Margin="0,10,0,0" Visibility="Collapsed"/>
        </StackPanel>
    </Grid>
</UserControl>
